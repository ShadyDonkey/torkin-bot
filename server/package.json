{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "auth:generate": "bunx @better-auth/cli generate --output ./zenstack/schemas/better-auth.zmodel --config ./lib/auth.ts",
    "bot:build": "dressed build",
    "db:migrate:create": "DATABASE_URL=postgresql://postgres:secret@localhost:5432/migrate bun zen migrate dev --create-only",
    "db:migrate:reset": "DATABASE_URL=postgresql://postgres:secret@localhost:5432/migrate bun zen migrate reset",
    "lib:tmdb:generate-schema": "openapi-typescript https://developer.themoviedb.org/openapi/tmdb-api.json -o ./lib/tmdb/schema.d.ts",
    "lib:tvdb:generate-schema": "openapi-typescript https://thetvdb.github.io/v4-api/swagger.yml -o ./lib/tvdb/schema.d.ts"
  },
  "dependencies": {
    "@dressed/matcher": "^1.4.0",
    "@dressed/react": "^1.8.1",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/static": "^1.4.7",
    "@keyv/redis": "^5.1.5",
    "@lukeed/ms": "^2.0.2",
    "@sinclair/typebox": "^0.34.47",
    "@sindresorhus/slugify": "^3.0.0",
    "@tanstack/react-query": "^5.90.16",
    "@zenstackhq/better-auth": "^3.1.1",
    "@zenstackhq/cli": "^3.1.1",
    "@zenstackhq/orm": "^3.1.1",
    "await-to-js": "^3.0.0",
    "better-auth": "^1.4.10",
    "cacheable": "^2.3.1",
    "camelcase-keys": "^10.0.1",
    "croner": "^9.1.0",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.37",
    "discord-fmt": "^1.0.1",
    "dressed": "^1.10.2-canary.3.2",
    "elysia": "^1.4.21",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.9.0",
    "keyv": "^5.5.5",
    "ky": "^1.14.2",
    "kysely": "~0.28.9",
    "lru-cache": "^11.2.4",
    "openapi-typescript": "^7.10.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-loki": "^3.0.0",
    "pino-pretty": "^13.1.3",
    "pino-std-serializers": "^7.0.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7"
  }
}
